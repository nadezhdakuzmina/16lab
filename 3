#include <iostream>
#include <cmath>
#include <vector>
using namespace std;


int main()
{
    int N, A, B, c ;
    cout << "введите N, A, B";
    cin >> N >> A >> B;
    vector <int> a(N);
    a[0]=A;
    a[1]=B;
    c=A+B;
    for (int i = 2; i < N; i++) {
        a[i]=c;
        c=c+a[i];
    }
    for (int i = 0; i < N; i++) {
        cout << a[i] << " ";
    }
    return 0;
}
