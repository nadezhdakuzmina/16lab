#include <iostream>
#include <cmath>
#include <vector>
using namespace std;


int main()
{
    int N, c=1 ;
    cout << "введите N";
    cin >> N ;
    cout << "введите массив";
    vector <int> a(N);
    for (int i = 0; i < N; i++) {
        cin >> a[i];
    }
    for (int i = 0; i < N/2; i++) {
        cout << a[i] << " " << a[N-c] <<" ";
        c++;
    }
    return 0;
}
