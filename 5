#include <iostream>
#include <cmath>
#include <vector>
using namespace std;


int main()
{
    int N;
    cout << "введите N";
    cin >> N ;
    cout << "введите массив";
    vector <int> a(N);
    for (int i = 0; i < N; i++) {
        cin >> a[i];
    }
    for (int i = 1; i < N; i=i+2) {
        cout << a[i] << " " ;

    }
    for (int i = 0; i < N; i=i+2) {
        cout << a[i] << " " ;

    }
    return 0;
}
